<jaxb:bindings
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns:jaxb="https://jakarta.ee/xml/ns/jaxb"
		jaxb:extensionBindingPrefixes="inheritance"
		jaxb:version="3.0">

	<jaxb:globalBindings>
		<jaxb:serializable uid="1" />
		<jaxb:javaType name="java.time.LocalDate" xmlType="xs:date"
					   parseMethod="ch.dvbern.oss.lib.iso20022.converter.LocalDateXMLConverter.parse"
					   printMethod="ch.dvbern.oss.lib.iso20022.converter.LocalDateXMLConverter.print" />
	</jaxb:globalBindings>
</jaxb:bindings>
